{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/expire-transactions",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/expire-points",
      "schedule": "0 0 * * *"
    }
  ]
}